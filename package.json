  },
  "homepage": "https://github.com/paulshryock/paul-shryock#readme",
  "scripts": {
    "prerebase": "HUSKY_SKIP_HOOKS=1",
    "start": "npm run build",
    "clean": "gulp clean",
    "lint": "gulp lint",
    "html": "gulp html",
    "svg": "gulp svg",
    "css": "gulp css",
    "javascript": "gulp javascript",
    "test": "gulp test",
    "build": "gulp build",
    "version": "gulp version && git add .",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test && sync-dotenv",
      "pre-push": "npm run lint && npm test"
    }
  },
  "sync-dotenv": {
    "preserve": [
      "BUILD_ENV",
      "SITE_URL",
      "TWITTER_API_KEY",
      "TWITTER_API_SECRET_KEY",
      "TWITTER_BEARER_TOKEN"
    ]
  },
  "browserslist": [
    "defaults"
  ],
